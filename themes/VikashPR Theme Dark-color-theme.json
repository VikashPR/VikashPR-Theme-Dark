            "settings": {
                "foreground": "#3ad900"
            }
        },
        {
            "name": "Support",
            "scope": "support",
            "settings": {
                "foreground": "#80ffbb"
            }
        },
        {
            "name": "Support Function",
            "scope": "support.function",
            "settings": {
                "foreground": "#ff9d00"
            }
        },
        {
