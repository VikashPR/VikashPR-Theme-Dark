            }
        },
        {
            "name": "[CSS] - Support",
            "scope": [
                "source.css support",
                "source.stylus support"
            ],
            "settings": {
                "foreground": "#a5ff90"
            }
        },
        {
            "name": "[CSS] - Constant",
            "scope": [
                "source.css constant",
