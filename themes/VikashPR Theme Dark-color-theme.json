    "tokenColors": [
        {
        "name": "Comment",
        "scope": [
            "comment",
            "punctuation.definition.comment"
        ],
        "settings": {
            "fontStyle": "italic",
            "foreground": "#e433b5"
        }
    },
    {
        "name": "Constant",
        "scope": "constant",
        "settings": {
            "foreground": "#ff62ab"
        }
    },
    {
        "name": "Entity",
        "scope": "entity",
        "settings": {
            "foreground": "#00bff9"
        }
    },
    {
        "name": "Invalid",
        "scope": "invalid",
        "settings": {
            "foreground": "#f44542",
            "fontStyle": "italic",
        }
    },
    {
        "name": "Storage Type Function",
        "scope": "storage.type.function",
        "settings": {
            "foreground": "#c792ea",
            "fontStyle": "italic"
        }
    },
    {
        "name": "Keyword",
        "scope": "keyword, storage.type.class, keyword.control.default.ts",
        "settings": {
            "foreground": "#c792ea"
        }
    },
    {
        "name": "Meta",
        "scope": "meta",
        "settings": {
